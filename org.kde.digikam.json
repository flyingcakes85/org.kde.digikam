{
  "id": "org.kde.digikam",
  "base": "io.qt.qtwebengine.BaseApp",
  "base-version": "5.15",
  "runtime": "org.kde.Platform",
  "runtime-version": "5.15",
  "sdk": "org.kde.Sdk",
  "command": "digikam",
  "finish-args": [
    "--device=all",
    "--filesystem=xdg-pictures",
    "--filesystem=/media",
    "--filesystem=/run/media",
    "--share=ipc",
    "--share=network",
    "--socket=cups",
    "--socket=wayland",
    "--socket=x11",
    "--system-talk-name=org.freedesktop.Avahi",
    "--system-talk-name=org.freedesktop.UDisks2",
    "--env=DK_PLUGIN_PATH=/app/lib/plugins/digikam"
  ],
  "add-extensions": {
    "org.freedesktop.Platform.ffmpeg-full": {
      "directory": "lib/ffmpeg",
      "add-ld-path": ".",
      "version": "20.08",
      "autodownload": true,
      "autodelete": false
    }
  },
  "rename-icon": "digikam",
  "cleanup": [
    "/include",
    "/lib/cmake",
    "/lib/libexec",
    "/lib/pkgconfig",
    "/share/doc",
    "/share/eigen3",
    "/share/man",
    "/share/pkgconfig",
    "*.la"
  ],
  "cleanup-commands": [
    "mkdir -p ${FLATPAK_DEST}/lib/ffmpeg",
    "/app/cleanup-BaseApp.sh"
  ],
